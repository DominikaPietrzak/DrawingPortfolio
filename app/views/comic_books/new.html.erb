<div class = "admin-container">
  <div class = "admin-row">
    <div class = "admin-column">
      <h1> ADD NEW COMIC</h1>
    </div>
  </div>
  <div class = "admin-row">
    <div class = "admin-column">
      <%= form_for @comic_book, html: { multipart: true}  do |f| %>

        <%= f.label :title %>
        <%= f.text_field :title %></br>

        <%= f.label :comiccover %>
        <%= f.file_field :comiccover %></br>

        <%= f.submit %>

      <% end %>
      <%= link_to 'Back', comic_books_path %>
    </div>
  </div>
</div>
